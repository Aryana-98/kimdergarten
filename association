package roomassso;


public class room{

    String name;
    Student allstudents[];

    public room(String name, Student students[]) {
        this.name = name;

        this.allstudents = students;
    }

    public Student[] getstudents() {
        return allstudents;
    }

public class Student {

    String name;
    String RoomName;

    public Student(String name, String RoomName) {
        this.name = name;
        this.RoomName = RoomName;
    }
}
    public static void main(String[] args) {
        Student s1 = new Student("anjila", "moon");
      Student s2= new Student("haval", "fruit");

        
    }   

}

